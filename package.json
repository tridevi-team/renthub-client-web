{
  "name": "rent-hub",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "description": "Hệ thống Quản lý Nhà trọ",
  "packageManager": "bun@1.1.22",
  "scripts": {
    "gen:pwa-assets": "pwa-assets-generator --preset minimal-2023 public/favicon.svg",
    "clean:biome": "biome clean",
    "check": "biome check --css-linter-enabled=true ./src",
    "check:fix": "biome check --css-linter-enabled=true --write ./src",
    "ci": "biome ci --css-linter-enabled=true ./src",
    "typecheck": "tsc --noEmit",
    "test:install": "playwright install --with-deps chromium",
    "regression": "run-p check typecheck test",
    "dev": "DEBUG=vite-plugin-pwa SW_DEV=false vite",
    "dev:clean": "rimraf dev-dist && DEBUG=vite-plugin-pwa SW_DEV=false vite --force",
    "dev:claims": "rimraf dev-dist && DEBUG=vite-plugin-pwa SW_DEV=true CLAIMS=true SW=true vite --force",
    "dev:destroy": "rimraf dev-dist && DEBUG=vite-plugin-pwa SW_DEV=true SW_DESTROY=true vite --force",
    "dev:claims-destroy": "rimraf dev-dist && DEBUG=vite-plugin-pwa SW_DEV=true CLAIMS=true SW_DESTROY=true SW=true vite --force",
    "build-only": "vite build",
    "build-only:staging": "vite build --mode staging",
    "build": "DEBUG=vite-plugin-pwa BASE_URL=/ SOURCE_MAP=true bun build-only",
    "build:staging": "DEBUG=vite-plugin-pwa BASE_URL=/ SOURCE_MAP=true bun build-only:staging",
    "build:reloadsw": "DEBUG=vite-plugin-pwa BASE_URL=/ SOURCE_MAP=true RELOAD_SW=true bun build-only",
    "build:claims": "DEBUG=vite-plugin-pwa BASE_URL=/ SOURCE_MAP=true CLAIMS=true bun build-only",
    "build:reloadsw-claims": "DEBUG=vite-plugin-pwa BASE_URL=/ SOURCE_MAP=true RELOAD_SW=true CLAIMS=true bun build-only",
    "build:sw": "rimraf dist && DEBUG=vite-plugin-pwa BASE_URL=/ SOURCE_MAP=true SW=false bun build-only",
    "build:sw-reloadsw": "DEBUG=vite-plugin-pwa BASE_URL=/ SOURCE_MAP=true SW=true RELOAD_SW=true bun build-only",
    "build:sw-claims": "DEBUG=vite-plugin-pwa BASE_URL=/ SOURCE_MAP=true SW=true CLAIMS=true bun build-only",
    "build:sw-reloadsw-claims": "DEBUG=vite-plugin-pwa BASE_URL=/ SOURCE_MAP=true SW=true RELOAD_SW=true CLAIMS=true bun build-only",
    "build:destroy": "DEBUG=vite-plugin-pwa BASE_URL=/ SOURCE_MAP=true SW_DESTROY=true bun build-only",
    "build:reloadsw-destroy": "DEBUG=vite-plugin-pwa BASE_URL=/ SOURCE_MAP=true RELOAD_SW=true SW_DESTROY=true bun build-only",
    "build:claims-destroy": "DEBUG=vite-plugin-pwa BASE_URL=/ SOURCE_MAP=true CLAIMS=true SW_DESTROY=true bun build-only",
    "build:reloadsw-claims-destroy": "DEBUG=vite-plugin-pwa BASE_URL=/ SOURCE_MAP=true RELOAD_SW=true CLAIMS=true SW_DESTROY=true bun build-only",
    "build:sw-destroy": "rimraf dist && DEBUG=vite-plugin-pwa BASE_URL=/ SOURCE_MAP=true SW=true SW_DESTROY=true bun build-only",
    "build:sw-reloadsw-destroy": "DEBUG=vite-plugin-pwa BASE_URL=/ SOURCE_MAP=true SW=true RELOAD_SW=true SW_DESTROY=true bun build-only",
    "build:sw-claims-destroy": "DEBUG=vite-plugin-pwa BASE_URL=/ SOURCE_MAP=true SW=true CLAIMS=true SW_DESTROY=true bun build-only",
    "build:sw-reloadsw-claims-destroy": "DEBUG=vite-plugin-pwa BASE_URL=/ SOURCE_MAP=true SW=true RELOAD_SW=true CLAIMS=true SW_DESTROY=true bun build-only",
    "serve": "serve dist",
    "start": "run-s build serve",
    "start:staging": "run-s build:staging serve",
    "start:reloadsw": "run-s bun build:reloadsw serve",
    "start:claims": "run-s bun build:claims serve",
    "start:reloadsw-claims": "run-s bun build:reloadsw-claims serve",
    "start:sw": "run-s bun build:sw serve",
    "start:sw-reloadsw": "run-s bun build:sw-reloadsw serve",
    "start:sw-claims": "run-s bun build:sw-claims serve",
    "start:sw-reloadsw-claims": "run-s bun build:sw-reloadsw-claims serve",
    "start:destroy": "run-s bun build:destroy serve",
    "start:reloadsw-destroy": "run-s bun build:reloadsw-destroy serve",
    "start:claims-destroy": "run-s bun build:claims-destroy serve",
    "start:reloadsw-claims-destroy": "run-s bun build:reloadsw-claims-destroy serve",
    "start:sw-destroy": "run-s bun build:sw-destroy serve",
    "start:sw-reloadsw-destroy": "run-s bun build:sw-reloadsw-destroy serve",
    "start:sw-claims-destroy": "run-s bun build:sw-claims-destroy serve",
    "start:sw-reloadsw-claims-destroy": "run-s build:sw-reloadsw-claims-destroy serve",
    "preview": "vite preview",
    "build-and-preview": "run-s build:sw preview",
    "create:locale": "tsx scripts/create-locale.ts"
  },
